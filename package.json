{
  "name": "osint-platform",
  "version": "4.0.0",
  "description": "Advanced OSINT Investigation Platform with Enhanced UI, Dynamic JS Handling, and NLP Analysis",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build:css": "npx tailwindcss -i ./public/styles/input.css -o ./public/styles/output.css --minify",
    "watch:css": "npx tailwindcss -i ./public/styles/input.css -o ./public/styles/output.css --watch",
    "test": "jest",
    "lint": "eslint . --ext .js",
    "format": "prettier --write \"**/*.js\"",
    "dev": "nodemon server.js"
  },
  "dependencies": {
    "express": "^4.19.2",
    "axios": "^1.7.2",
    "cheerio": "^1.0.0",
    "uuid": "^10.0.0",
    "tough-cookie": "^4.1.4",
    "puppeteer": "^23.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.7"
  },
  "devDependencies": {
    "nodemon": "^3.1.4",
    "jest": "^29.7.0",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-react": "^7.24.7"
  },
  "engines": {
    "node": ">=20"
  }
}
